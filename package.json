    "lazysizes": "^5.3.2",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "node-sass-glob-importer": "^5.3.2",
    "popper.js": "^1.16.0",
    "postcss-loader": "^6.2.1",
    "precss": "^4.0.0",
    "raw-loader": "^4.0.2",
    "stats-webpack-plugin": "^0.7.0",
