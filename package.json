{
  "name": "onxrp",
  "version": "1.0.0",
  "author": "Cemtrexlabs Team",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "clean": "rm -rf themes/onxrp/dist/*",
    "start": "NODE_ENV=development NODE_SITE=onxrp node ./webpack/start.js",
    "build": "NODE_ENV=production NODE_SITE=onxrp node ./webpack/build.js",
    "sniff:setup": "chmod +x sniff_setup.sh && ./sniff_setup.sh",
    "docker:start": "docker-compose down && COMPOSE_HTTP_TIMEOUT=200 docker-compose up --build",
    "docker:setup": "docker exec -it onxrp-php sh -c /setup.sh",
    "docker:shell": "docker exec -it onxrp-php /bin/bash",
    "docker:ip": "docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' onxrp-nginx",
    "phpcs": "phpcs --standard=WordPress-VIP-Go -sp --basepath=. --ignore=node_modules/* themes/onxrp*",
    "update-deps": "rm -rf node_modules && yarn install",
    "docker:ip:update": "IP=$(docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' onxrp-nginx) && HOST_ST=\"'$IP onxrp.local '\" && docker exec onxrp-php /bin/bash -c \"echo $HOST_ST  >> /etc/hosts\""
  },
  "dependencies": {
    "@wordpress/babel-plugin-makepot": "^4.2.0",
    "@wordpress/babel-preset-default": "^6.5.0",
    "@wordpress/browserslist-config": "^4.1.0",
